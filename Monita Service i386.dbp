#! /bin/bash -e

echo "chmod +x /etc/init.d/monita-service"
chmod +x "/etc/init.d/monita-service"

echo "chown $user:$user /etc/init.d/monita-service"
chown $user:$user "/etc/init.d/monita-service"

echo "update-rc.d monita-service defaults"
update-rc.d monita-service defaults

read -p "Ingin menjalankan monita-service sekarang ?? [y/N]?" yn_service
case $yn_service in
  [Yy]* ) 
    echo "service monita-service start"
    service monita-service start
    echo "service monita-service status"
    service monita-service status
    read -r -p "installasi selesai [ENTER]"
