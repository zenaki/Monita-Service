then
    echo "add-apt-repository deb http://119.18.154.235:8077/html/repo/qt561-trusty trusty main";
    add-apt-repository 'deb http://119.18.154.235:8077/html/repo/qt561-trusty trusty main';
    read -p "Ingin Update Repository List ?? [y/N] "
    if [[ $REPLY =~ ^[Yy]$ ]];
    then
        echo "apt-get update";
        apt-get update;
        echo "Update Repository List selesai ..";
